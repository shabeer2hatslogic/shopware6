<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="SwagMigrationAssistant\Migration\Logging\SwagMigrationLoggingDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_logging"/>
        </service>

        <service id="SwagMigrationAssistant\Migration\Run\SwagMigrationRunDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_run"/>
        </service>

        <service id="SwagMigrationAssistant\Migration\Run\MigrationProgressFieldSerializer">
            <argument type="service" id="validator"/>
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\DefinitionInstanceRegistry"/>

            <tag name="shopware.field_serializer"/>
        </service>

        <service id="SwagMigrationAssistant\Migration\Run\PremappingFieldSerializer">
            <argument type="service" id="validator"/>
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\DefinitionInstanceRegistry"/>

            <tag name="shopware.field_serializer"/>
        </service>

        <service id="SwagMigrationAssistant\Migration\Data\SwagMigrationDataDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_data"/>
        </service>

        <service id="SwagMigrationAssistant\Migration\Mapping\SwagMigrationMappingDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_mapping"/>
        </service>

        <service id="SwagMigrationAssistant\Migration\Media\SwagMigrationMediaFileDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_media_file"/>
        </service>

        <service id="SwagMigrationAssistant\Migration\Setting\GeneralSettingDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_general_setting"/>
        </service>

        <service id="SwagMigrationAssistant\Migration\Connection\SwagMigrationConnectionDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_connection"/>
        </service>
    </services>
</container>
